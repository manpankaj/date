        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".disclaimerActivity" android:label="DISCLAIMER"> </activity>
        <activity android:name=".loginwActivity" android:label="WORKER LOGIN"> </activity>
        <activity android:name=".logineActivity" android:label="EMPLOYER LOGIN"> </activity>
        <activity android:name=".registrationwActivity" android:label="WORKER REGISTRATION"> </activity>
        <activity android:name=".registrationeActivity" android:label="EMPLOYER REGISTRATION"> </activity>
        <activity android:name=".WelcomewActivity" android:label="WELCOME WORKER"> </activity>
